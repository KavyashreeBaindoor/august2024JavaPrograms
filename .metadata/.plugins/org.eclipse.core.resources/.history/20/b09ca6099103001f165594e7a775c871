package first;

public class Main {
	 public static void main(String args[]) {
	     	child2 c=new child2(87);
	      System.out.println(c.l);
	     
	     }
}


 class Box {
	int l;int w;int h;
	
	Box(){
	   this.l=1;
	   this.w=2;
	   this.h=3;
   }
   
   Box(int l,int w,int h){
	   this.l=l;
	   this.w=w;
	   this.h=h;
   }
}


 class child extends Box{
	int weight;
	child(int w){
		super(909,5,7);
		this.weight=w;
	}	
}
 
 class child2 extends child{
	int price;
	child2(int p){
		super(90);
		this.price=p;
		super.l=900;
	}	
}