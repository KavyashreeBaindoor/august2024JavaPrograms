package first;

public class Main {
	 public static void main(String args[]) {
	     	child c=new child(87);
	      System.out.println(c.l);
	     
	     }
}


 class Box {
	int l;int w;int h;
	
	Box(){
	   this.l=1;
	   this.w=2;
	   this.h=3;
   }
	
	public void message() {
		System.out.println("hey");
	}
   }
 
class child extends Box{
	int weight;
	child(int w){
		this.weight=w;
	}	
	public void message() {
		System.out.println("hey i am child");
	}
}
 
 	
