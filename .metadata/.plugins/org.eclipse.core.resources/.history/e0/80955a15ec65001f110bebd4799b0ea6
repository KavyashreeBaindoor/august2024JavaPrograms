package August2024;

public class countVowelsInString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		String a="kavya";
		String b="aeiou";
		int count=0;
		
		String[] arr=b.split("");
		
		
		for(int i=0;i<a.length();i++) {
		if(arr[i].contains(a))
				count++;
		}
		
		
		System.out.println(count);
		
		
	}

}
