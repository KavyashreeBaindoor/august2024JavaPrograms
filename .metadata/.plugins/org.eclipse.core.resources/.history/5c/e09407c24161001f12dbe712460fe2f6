package August2024;

public class swapTwoNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int num1=9;
		int num2=1;
		
		
		
		int temp=num1; //9
		num1=num2;
		num2=temp;
		
		System.out.println(num1+" "+num2);
		
		//without 3rd variable
		
		int a=10;
		int b=20;
		
		 a=a+b;  //30
		 b=a-b;//10
		 a=a-b; //20
		 
		 System.out.println(a+" "+b);
		 
		
		

	}

}
