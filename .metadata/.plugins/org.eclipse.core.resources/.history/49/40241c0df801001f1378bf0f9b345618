package first;

public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Student s1=new Student();
		System.out.println(s1.name);
		System.out.println(s1.rollno);
		System.out.println(s1.age);
		

	}

}

class Student{
	
	Student(){
		this.name=name;
		this.age=age;
		this.rollno=rollno;
	}
	
	String name;
	int rollno;
	int age;
	
}
